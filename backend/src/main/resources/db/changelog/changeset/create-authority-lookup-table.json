{
  "databaseChangeLog": [
    {
      "changeSet": {
        "id": "create-authority-lookup-table",
        "author": "Nikita Lepikhin",
        "changes": [
          {
            "createTable": {
              "tableName": "authority_lookup",
              "columns": [
                {
                  "column": {
                    "name": "id",
                    "type": "int",
                    "autoIncrement": true,
                    "constraints": {
                      "primaryKey": true,
                      "nullable": false
                    }
                  }
                },
                {
                  "column": {
                    "name": "uuid",
                    "type": "uuid",
                    "constraints": {
                      "nullable": false,
                      "unique": true
                    }
                  }
                },
                {
                  "column": {
                    "name": "authority_value",
                    "type": "varchar(16)",
                    "constraints": {
                      "nullable": false,
                      "unique": true
                    }
                  }
                }
              ]
            }
          }
        ]
      }
    }
  ]
}
